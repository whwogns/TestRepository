package ch12.exam05;

public class Calculator {//공유객체 설정
	
	//Field
	private int memory;
	//Constructor
	//Method

	public int getMemory() {
		return memory;
	}

	public void setMemory(int memory){
		this.memory = memory;
		try {
			Thread.sleep(2000);
		} catch (InterruptedException e) {
			
		}
		System.out.println(Thread.currentThread().getName() + ":"+this.memory);
	}
}
