package ch12.exam02;

import java.awt.Toolkit;

public class BeepPrintExample {
	public static void main(String[] args) {
		//소리를 내는 코드
		BeepThread beepThread = new BeepThread();
		beepThread.start();
		
		//프린트를 하는 코드
		for(int i=0;i<5;i++){
			System.out.println("띵");
			try{
				Thread.sleep(500);//천분의 1초로 휴식
				}catch(InterruptedException e){
					
				}
		
		}
		
	}
}
//How2