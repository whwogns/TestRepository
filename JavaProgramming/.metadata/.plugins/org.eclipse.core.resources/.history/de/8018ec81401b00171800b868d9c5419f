package ch12.exam05;

public class User2 extends Thread{
	//Field 
	private Calculator calculator;
	
	//Constructor
	public User2(){
		super("User1");
	}
	//Method
	@Override
	public void run() {
		calculator.setMemory(100);
	}
	
	public void setCalculator(Calculator calculator) {
		this.calculator = calculator;
	}
}
